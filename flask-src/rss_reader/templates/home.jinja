{% extends "base.jinja" %}
{% block body %}
<p>
  Welcome to my RSS reader
</p>

<!-- https://htmx.org/examples/file-upload/ -->
<input required type='file' name='file' accept="text/xml" form="upload">

<form id="upload" hx-post="/blogs/import" enctype="multipart/form-data" hx-target="#feedback">
  <button>Upload</button>
</form>

<p id="feedback">
</p>
  
{% endblock %}
